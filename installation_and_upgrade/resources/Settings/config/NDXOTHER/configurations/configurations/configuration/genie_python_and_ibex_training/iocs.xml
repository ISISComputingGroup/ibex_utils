<?xml version="1.0" ?>
<iocs xmlns="http://epics.isis.rl.ac.uk/schema/iocs/1.0" xmlns:ioc="http://epics.isis.rl.ac.uk/schema/iocs/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
	<ioc autostart="true" name="SIMPLE" restart="true" simlevel="none">
		<macros/>
		<pvs/>
		<pvsets/>
	</ioc>
	<ioc autostart="true" name="MCLEN_01" restart="true" simlevel="devsim">
		<macros>
			<macro description="Velocity, units/s, where length units are defined in the corresponding MSTP macro (default: 1)" name="VELO1" pattern="^[0-9]*\.?[0-9]*$" value="100"/>
			<macro name="NAME1" value="TRAIN"/>
			<macro name="AXIS1" value="yes"/>
			<macro name="MTRCTRL" value="1"/>
		</macros>
		<pvs/>
		<pvsets/>
	</ioc>
</iocs>
